{
  "license": "",
  "options": {
    "camera" : {
      "captureResolution" : "1080p",
      "zoomGesture": true
    },
    "flash" : {
      "mode": "manual",
      "alignment": "bottom_right",
      "imageOn": "flash_on",
      "imageOff": "flash_off"
    },
    "viewPlugin" : {
      "plugin":{
        "id":"ID",
        "idPlugin": {
          "universalIdConfig": {
            "alphabet" : "cyrillic",
            "faceDetection": true,
            "allowedLayouts": {
              "mrz": [],
              "drivingLicense": [],
              "idFront": []
             },
             "drivingLicense": {
               "lastName": {"scanOption": 0, "minConfidence": 40},
               "lastName@cyr": {"scanOption": 0, "minConfidence": 40},
               "firstName": {"scanOption": 0, "minConfidence": 40},
               "firstName@cyr": {"scanOption": 0, "minConfidence": 40},
               "dateOfBirth": {"scanOption": 0, "minConfidence": 50},
               "placeOfBirth": {"scanOption": 1, "minConfidence": 50},
               "dateOfIssue": {"scanOption": 0, "minConfidence": 50},
               "dateOfExpiry": {"scanOption": 1, "minConfidence": 50},
               "authority": {"scanOption": 1, "minConfidence": 30},
               "documentNumber": {"scanOption": 0, "minConfidence": 40},
               "address": {"scanOption": 1},
               "address@cyr": {"scanOption": 1},
               "nationality": {"scanOption": 1},
               "nationality@cyr": {"scanOption": 1}
              },
              "idFront": {
                "lastName": {"scanOption": 0, "minConfidence": 40},
                "lastName@cyr": {"scanOption": 0, "minConfidence": 40},
                "firstName": {"scanOption": 0, "minConfidence": 40},
                "firstName@cyr": {"scanOption": 0, "minConfidence": 40},
                "dateOfBirth": {"scanOption": 0, "minConfidence": 50},
                "placeOfBirth": {"scanOption": 1, "minConfidence": 50},
                "dateOfIssue": {"scanOption": 0, "minConfidence": 50},
                "dateOfExpiry": {"scanOption": 1, "minConfidence": 50},
                "authority": {"scanOption": 1, "minConfidence": 30},
                "documentNumber": {"scanOption": 0, "minConfidence": 40},
                "address": {"scanOption": 1},
                "address@cyr": {"scanOption": 1},
                "nationality": {"scanOption": 1},
                "nationality@cyr": {"scanOption": 1}
              }
            }
          }
      },
      "cutoutConfig" : {
        "style": "animated_rect",
        "maxWidthPercent": "90%",
        "maxHeightPercent": "90%",
        "alignment": "center",
        "strokeWidth": 3,
        "cornerRadius": 8,
        "strokeColor": "FFFFFF",
        "outerColor": "000000",
        "outerAlpha": 0.3,
        "ratioFromSize" : {
          "width": 50,
          "height": 31
        },
        "cropPadding": {
          "x": -50,
          "y": -50
        },
        "cropOffset": {
          "x": 0,
          "y": 0
        },
        "feedbackStrokeColor": "0099FF"
      },
      "scanFeedback" : {
        "style": "CONTOUR_RECT",
        "visualFeedbackRedrawTimeout": 100,
        "strokeColor": "0099FF",
        "fillColor" : "220099FF",
        "beepOnResult": true,
        "vibrateOnResult": true,
        "strokeWidth": 2
      },
      "cancelOnResult" : true
    }
  }
  
}
